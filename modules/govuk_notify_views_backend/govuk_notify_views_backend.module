<?php

/**
 * @file
 * Contains govuk_notify_views_backend.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function govuk_notify_views_backend_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the govuk_notify_views_backend module.
    case 'help.page.govuk_notify_views_backend':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Views Backend for GovUK Notify') . '</p>';
      return $output;

    default:
  }
}
